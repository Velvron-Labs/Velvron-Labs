<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8b5cf6;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="#001233"/>
  
  <!-- Gradient overlay -->
  <rect width="800" height="600" fill="url(#grad1)" opacity="0.1"/>
  
  <!-- Grid pattern -->
  <defs>
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#3b82f6" stroke-width="0.5" opacity="0.2"/>
    </pattern>
  </defs>
  <rect width="800" height="600" fill="url(#grid)"/>
  
  <!-- Main content area -->
  <g opacity="0.3">
    <!-- Large center circle -->
    <circle cx="400" cy="300" r="150" fill="none" stroke="#3b82f6" stroke-width="2"/>
    <circle cx="400" cy="300" r="120" fill="none" stroke="#8b5cf6" stroke-width="2"/>
    <circle cx="400" cy="300" r="90" fill="none" stroke="#06b6d4" stroke-width="2"/>
    
    <!-- Corner accents -->
    <rect x="50" y="50" width="100" height="100" fill="none" stroke="#3b82f6" stroke-width="2"/>
    <rect x="650" y="50" width="100" height="100" fill="none" stroke="#8b5cf6" stroke-width="2"/>
    <rect x="50" y="450" width="100" height="100" fill="none" stroke="#06b6d4" stroke-width="2"/>
    <rect x="650" y="450" width="100" height="100" fill="none" stroke="#3b82f6" stroke-width="2"/>
  </g>
  
  <!-- Text -->
  <text x="400" y="280" font-family="Arial, sans-serif" font-size="28" fill="#3b82f6" text-anchor="middle" opacity="0.6">
    Project
  </text>
  <text x="400" y="320" font-family="Arial, sans-serif" font-size="28" fill="#8b5cf6" text-anchor="middle" opacity="0.6">
    Placeholder
  </text>
</svg>
